            </Panel>
             <Panel
                   width="300px"
                   height="220px"
                   class="Panel_2">
            <content>
                <Button
                    text="{/Katalog/3/name}"
                    press=".onOpenDialog" 
                    class="Button2"
                    type="Unstyled"/>
