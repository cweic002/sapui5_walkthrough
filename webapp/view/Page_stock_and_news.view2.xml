<mvc:View
          controllerName="sap.ui.demo.walkthrough.controller.Page_stock_and_news"
          xmlns="sap.m"
          xmlns:mvc="sap.ui.core.mvc">
    <Panel
           headerText="{i18n>Контент_страница}"
           class="Panel2"
           width="1250px"
           height="550px"
           backgroundDesign="Solid"
           style="Clear"
           busyIndicatorSize="Auto">
        <content>
            <Panel
                   width="300px"
                   height="220px"
                   class="Panel_2">
            <content>
                <Button
                    text="{/Katalog/0/name}"
                    press=".onOpenDialog" 
                    class="Button2"
                    type="Unstyled"/>
                <Text text ="{/Katalog/0/name2}"/>
                <Image src="{/Katalog/0/image}"/>
                <Text text="Цена {/Katalog/0/prise}"></Text>
                </content>
            </Panel>
             <Panel
                   width="300px"
                   height="220px"
                   class="Panel_2">
            <content>
