<mvc:View
          controllerName="sap.ui.demo.walkthrough.controller.Stell_Store"
          xmlns="sap.m"
          xmlns:mvc="sap.ui.core.mvc">
    <Panel
           headerText="{i18n>helloPanelTitle}"
           class="Panel"
           width="300px"
           height="750px"
           backgroundDesign="Solid"
           style="Clear">
        <content>
            <Bar enableFlexBox="true"
                 design="Footer"
                 class="Bar">
                <contentLeft>
                    <Button
                            text="{i18n>Button_сatalog}"
                            press=".Новый_каталог"
                            class="Button"
                            type="Unstyled"/>
                </contentLeft>
            </Bar>
            <Bar 
                 enableFlexBox="true"
                 design="Footer"
                 class="Bar">
                <contentLeft>
                    <Button
                            text="{i18n>Button_сustom_designs}"
                            press=".onShowHello"
                            class="Button"
                            type="Unstyled"/>
                </contentLeft>
            </Bar>
            <Bar 
                 enableFlexBox="true"
                 design="Footer"
                 class="Bar">
                <contentLeft>
                    <Button
                            text="{i18n>Button_request_call}"
                            press=".onShowHello"
                            class="Button"
                            type="Unstyled"/>
                </contentLeft>
            </Bar>
            <Bar 
                 enableFlexBox="true"
                 design="Footer"
                 class="Bar">
                <contentLeft>
                    <Button
                            text="{i18n>Button_сontacts}"
                            press=".onShowHello"
                            class="Button"
                            type="Unstyled"/>
                </contentLeft>
            </Bar>
            <Bar 
                 enableFlexBox="true"
                 design="Footer"
                 class="Bar">
                <contentLeft>
                    <Button
                            text="{i18n>Button_admin}"
                            press=".onShowHello"
                            class="Button"
                            type="Unstyled"/>
                </contentLeft>
            </Bar>
        </content>
    </Panel>
</mvc:View>